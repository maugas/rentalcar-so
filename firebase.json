{
  "hosting": {
    "public": "dist/rentalcar-so",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],

    "rewrites": [
      {
        "source": "/en/**",
        "destination": "/en/index.html"
      },
      {
        "source": "/so/**",
        "destination": "/so/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/en/index.html",
        "type": 301
      }
    ]
  }
}
